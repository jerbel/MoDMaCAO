<?xml version="1.0" encoding="ASCII"?>
<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="ansibleconfiguration" scheme="http://schemas.modmacao.org/occi/ansible#" description="AnsibleConfigurationExtension">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <import href="http://schemas.ogf.org/occi/infrastructure#/"/>
  <import href="http://schemas.modmacao.org/modmacao#/"/>
  <mixins name="AnsibleEndpoint" scheme="http://schemas.modmacao.org/occi/ansible#">
    <applies href="http://schemas.ogf.org/occi/infrastructure#//@kinds[term='networkinterface']"/>
  </mixins>
  <mixins name="AnsibleConfiguration" scheme="http://schemas.modmacao.org/occi/ansible#">
    <attributes name="ansible.privatekey" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="ansible.remoteuser" mutable="true">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <depends href="http://schemas.modmacao.org/modmacao#//@mixins[term='component']"/>
  </mixins>
</occi:Extension>
